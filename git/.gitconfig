[include]
    path = ".config/git/config"

[include]
    path = ".config/git/config-private"
[core]
	quotepath = false
	commitGraph = true
[gc]
	writeCommitGraph = true
[credential]
	helper = store
[init]
	defaultbranch = main
[merge]
	conflictstyle = diff3
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[gpg]
	format = ssh
[user]
	signingkey = /home/jess/.ssh/id_ed25519.pub
[commit]
	gpgsign = true
[tag]
	gpgsign = true
