[include]
    path = ".config/git/config"

[include]
    path = ".config/git/config-private"
[core]
	quotepath = false
	commitGraph = true
[gc]
	writeCommitGraph = true
[credential]
	helper = store
[init]
	defaultBranch = main
[merge]
	conflictstyle = diff3
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
